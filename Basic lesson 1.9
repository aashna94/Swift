import UIKit

enum Rank : Int {
    case ace = 1
    case two, three, four, five, six, seven, eight, nine
    case jack, queen, king
    
    func description() -> String {
        switch self {
        case .ace:
            return "ace"
            break
        case .king:
            return "king"
            break
        case .queen:
            return "queen"
            break
        default:
            return String(self.rawValue)
        }
    }
}

let ace = Rank.ace
print(ace)
print(ace.rawValue)
let k = Rank.king
print(k)
print(k.rawValue)


